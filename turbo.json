{
    "$schema": "https://turbo.build/schema.json",
    "globalEnv": ["NODE_ENV"],
    "envMode": "loose",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
